{
  "name": "zip-front-app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "serve:ssr:zip-front-app": "node dist/zip-front-app/server/server.mjs",
    "prepare": "husky",
    "release": "standard-version",
    "lint": "eslint \"**/*.{js,ts}\" --fix --quiet",
    "i18n:extract:es": "ngx-translate-extract -i ./src -o ./src/assets/i18n/es.json -k -f json -s -c",
    "i18n:extract:en": "ngx-translate-extract -i ./src -o ./src/assets/i18n/en.json -k -f json -s -c"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "18.2.4",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "18.2.4",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/service-worker": "^18.2.0",
    "@angular/ssr": "^18.2.4",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@supabase/supabase-js": "2.45.4",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.0.0",
    "express": "^4.18.2",
    "globals": "^15.9.0",
    "js-cookie": "^3.0.5",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.4",
    "@angular-eslint/builder": "^18.3.1",
    "@angular-eslint/eslint-plugin": "^18.3.1",
    "@angular-eslint/eslint-plugin-template": "^18.3.1",
    "@angular-eslint/schematics": "^18.3.1",
    "@angular-eslint/template-parser": "^18.3.1",
    "@angular/cli": "^18.2.4",
    "@angular/compiler-cli": "^18.2.0",
    "@bartholomej/ngx-translate-extract": "^8.0.2",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-angular": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.13",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^9.10.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.4",
    "standard-version": "^9.5.0",
    "typescript": "~5.5.2",
    "typescript-eslint": "^8.5.0"
  }
}
